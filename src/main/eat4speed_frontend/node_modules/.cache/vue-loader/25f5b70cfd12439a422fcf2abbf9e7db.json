alse\"\r\n          class=\"elevation-1 pa-6\"\r\n          item-key=\"name\"\r\n          multi-sort\r\n      >\r\n        <template v-slot:top>\r\n          <v-container fluid>\r\n            <v-col cols=\"6\">\r\n              <v-row>\r\n                Liefertermin: {{ getTermin() }}\r\n              </v-row>\r\n              <v-row>\r\n                restliche Stationen: {{ getStationen() }}\r\n              </v-row>\r\n            </v-col>\r\n          </v-container>\r\n        </template>\r\n\r\n        <template v-slot:item.map=\"{ }\">\r\n          <GmapMap\r\n              :center=\"{lat:10, lng:10}\"\r\n              :zoom=\"7\"\r\n              map-type-id=\"terrain\"\r\n              style=\"width: 250px; height: 200px\"\r\n          >\r\n            <GmapMarker\r\n                :key=\"index\"\r\n                v-for=\"(m, index) in markers\"\r\n                :position=\"m.position\"\r\n                :clickable=\"true\"\r\n                :draggable=\"true\"\r\n                @click=\"center=m.position\"\r\n            />\r\n          </GmapMap>\r\n        </template>\r\n\r\n        <template v-slot:item.actions=\"{ item }\">\r\n          <v-btn\r\n              v-bind=\"attrs\"\r\n              v-on=\"on\"\r\n              color=\"blue\"\r\n              dark\r\n              rounded\r\n              @click=\"abholungBestätigen(item)\"\r\n          >Bestätigen</v-btn>\r\n        </template>\r\n      </v-data-table>\r\n    </v-card>\r\n  </v-main>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"FahrerFahrtenplan\",\r\n  methods: {\r\n    abholungBestätigen(id) {\r\n      id;\r\n    },\r\n    getTermin() {\r\n      return '12.12.2021';\r\n    },\r\n    getStationen() {\r\n      return 2;\r\n    },\r\n    convertToKM(meters) {\r\n      return meters / 1000;\r\n    }\r\n  },\r\n  mounted() {\r\n    this.data = [\r\n      {\r\n        station: \"1\",\r\n        beschreibung: \"Kurzbeschreibung 1\",\r\n        restaurantname: \"Restaurant 1\",\r\n        entfernung: \"1 km\"\r\n      },\r\n      {\r\n        station: \"2\",\r\n        beschreibung: \"Kurzbeschreibung 1\",\r\n        entfernung: \"2 km\"\r\n      }\r\n    ]\r\n  },\r\n  data() {\r\n    return {\r\n      data: [],\r\n      acceptDialog: false,\r\n      deleteDialog: false,\r\n      headers: [\r\n        {\r\n          text: 'Karte',\r\n          align: 'start',\r\n          sortable: false,\r\n          value: 'map'\r\n        },\r\n        {\r\n          text: 'Station',\r\n          value: 'station',\r\n          sortable: false\r\n        },\r\n        {\r\n          text: 'Beschreibung',\r\n          value: 'beschreibung',\r\n          sortable: false\r\n        },\r\n        {\r\n          text: 'Restaurantname',\r\n          value: 'restaurantname',\r\n          sortable: false\r\n        },\r\n        {\r\n          text: 'Entfernung',\r\n          value: 'entfernung',\r\n          sortable: false\r\n        },\r\n        {\r\n          value: 'actions',\r\n          sortable: false,\r\n          align: 'end'\r\n        },\r\n      ],\r\n    };\r\n  },\r\n};\r\n</script>\r\n"]}]}