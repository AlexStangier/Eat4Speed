ld>\r\n                          </v-col>\r\n                          <v-spacer></v-spacer>\r\n                          <v-col>\r\n                            <v-btn\r\n                              :disabled=\"!validateRegistration\"\r\n                              color=\"red\"\r\n                              dark\r\n                              rounded\r\n                              @click=\"validateRegistration\"\r\n                              >Weiter zur Verifikation</v-btn\r\n                            >\r\n                          </v-col>\r\n                        </v-row>\r\n                      </v-form>\r\n                    </v-card-text>\r\n                  </v-card>\r\n                </v-tab-item>\r\n                <v-tab-item>\r\n                  <v-card class=\"px-4\">\r\n                    <v-card-text>\r\n                      <v-form\r\n                        ref=\"verificationForm\"\r\n                        v-model=\"verificationValid\"\r\n                        lazy-validation\r\n                      >\r\n                        <v-row>\r\n                          <v-col cols=\"12\">\r\n                            <v-combobox\r\n                              label=\"Fahrzeug\"\r\n                              :items=\"vehicles\"\r\n                              v-model=\"vehicle\"\r\n                              :rules=\"[rules.required]\"\r\n                              required\r\n                            ></v-combobox>\r\n                          </v-col>\r\n                          <v-col cols=\"12\">\r\n                            <v-text-field\r\n                              v-model=\"driverLicense\"\r\n                              label=\"Führerschein\"\r\n                              :rules=\"[rules.required]\"\r\n                              required\r\n                            ></v-text-field>\r\n                          </v-col>\r\n                          <v-col cols=\"12\">\r\n                            <v-text-field\r\n                              v-model=\"licensePlate\"\r\n                              label=\"Fahrzeug(Modell / Schlüsselnummer)\"\r\n                              :rules=\"[rules.required]\"\r\n                              required\r\n                            ></v-text-field>\r\n                          </v-col>\r\n                          <v-col>\r\n                            <v-btn color=\"blue\" dark rounded @click=\"linkPaypal\"\r\n                              >Paypal Konto verknüpfen</v-btn\r\n                            >\r\n                          </v-col>\r\n                          <v-spacer></v-spacer>\r\n                          <v-col cols=\"12\">\r\n                            <v-checkbox\r\n                              label=\"AGB gelesen und akzeptiert\"\r\n                              v-model=\"agbAccepted\"\r\n                            ></v-checkbox>\r\n                          </v-col>\r\n                          <v-col class=\"text-left\">\r\n                            <v-btn\r\n                              color=\"red\"\r\n                              dark\r\n                              rounded\r\n                              @click=\"validateVerification\"\r\n                              >Fahrerkonto erstellen</v-btn\r\n                            >\r\n                          </v-col>\r\n                        </v-row>\r\n                      </v-form>\r\n                    </v-card-text>\r\n                  </v-card>\r\n                </v-tab-item>\r\n              </v-tabs>\r\n            </v-card>\r\n          </v-flex>\r\n        </v-layout>\r\n      </v-container>\r\n    </v-main>\r\n  </v-app>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"LoginDriver\",\r\n  computed: {\r\n    passwordMatch() {\r\n      return () =>\r\n        this.password === this.verify || \"Passwort muss übereinstimmen.\";\r\n    },\r\n  },\r\n  methods: {\r\n    validateLogin() {\r\n      if (this.$refs.loginForm.validate()) {\r\n        // submit form to server/API here...\r\n      }\r\n    },\r\n    validateRegistration() {\r\n      if (this.$refs.registrationForm.validate()) {\r\n        this.tab = 2;\r\n      }\r\n    },\r\n    validateVerification() {\r\n      if (this.$refs.verificationForm.validate()) {\r\n        // submit form to server/API here...\r\n      }\r\n    },\r\n    linkPaypal() {\r\n      // TODO:\r\n    },\r\n    reset() {\r\n      this.$refs.form.reset();\r\n    },\r\n    resetValidation() {\r\n      this.$refs.form.resetValidation();\r\n    },\r\n  },\r\n  data() {\r\n    return {\r\n      tab: 0,\r\n      tabs: [\r\n        { name: \"Login\", icon: \"mdi-account\" },\r\n        { name: \"Registrieren\", icon: \"mdi-account-outline\" },\r\n        { name: \"Verifikation\", icon: \"mdi-license\" }, // TODO: Hide tab head\r\n      ],\r\n      // Verification Tab\r\n      // TODO: retrieve valid vehicles from API\r\n      vehicles: [\"PKW\", \"Fahrrad\", \"Motorrad\"],\r\n      vehicle: \"\",\r\n      driverLicense: \"\",\r\n      licensePlate: \"\",\r\n      agbAccepted: false,\r\n      verificationValid: false,\r\n      // Registration Tab\r\n      registrationValid: true,\r\n      salutation: \"\",\r\n      firstName: \"\",\r\n      lastName: \"\",\r\n      street: \"\",\r\n      houseNumber: \"\",\r\n      place: \"\",\r\n      postCode: \"\",\r\n      email: \"\",\r\n      phoneNumber: \"\",\r\n      date: \"\",\r\n      password: \"\",\r\n      verify: \"\",\r\n      // Login Tab\r\n      loginValid: false,\r\n      loginPassword: \"\",\r\n      loginEmail: \"\",\r\n      loginEmailRules: [\r\n        (v) => !!v || \"Required\",\r\n        (v) => /.+@.+\\..+/.test(v) || \"E-Mail muss gültig sein\",\r\n      ],\r\n      emailRules: [\r\n        (v) => !!v || \"Required\",\r\n        (v) => /.+@.+\\..+/.test(v) || \"E-Mail muss gültig sein\",\r\n      ],\r\n      show1: false,\r\n      rules: {\r\n        required: (value) => !!value || \"Required.\",\r\n        min: (v) => (v && v.length >= 8) || \"Mindestens 8 Zeichen\",\r\n      },\r\n    };\r\n  },\r\n};\r\n</script>"],"sourceRoot":"src/views"}]}